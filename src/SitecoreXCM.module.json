        "path":"/sitecore/content/Applications/Content Editor/Ribbons/Chunks/SV Commerce Master Sync",
        "scope": "ItemAndDescendants",
        "database":"core"
      },
      {
        "name": "SyncRibbons",
        "path":"/sitecore/content/Applications/Content Editor/Ribbons/Ribbons/Default/Sv Commerce",
        "scope": "SingleItem",
        "database":"core"
      },
      {
        "name": "SyncStrips",
        "path":"/sitecore/content/Applications/Content Editor/Ribbons/Strips/SV Commerce",
        "scope": "ItemAndDescendants",
        "database":"core"
      },
      {
        "name": "Launchpad-Buttons",
        "path":"/sitecore/client/Applications/Launchpad/PageSettings/Buttons/SVCommerce",
        "scope": "ItemAndDescendants",
        "database":"core"
      }
    ]
