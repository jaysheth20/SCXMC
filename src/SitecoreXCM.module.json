{
  "$schema": "../.sitecore/schemas/ModuleFile.schema.json",
  "namespace": "Project",
  "items": {
    "includes": [
      {
        "name": "Site",
        "path":"/sitecore/content/sitecore-jss-app",
        "scope": "SingleItem"
      },
      {
        "name": "Home",
        "path":"/sitecore/content/sitecore-jss-app/home",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "Content",
        "path":"/sitecore/content/sitecore-jss-app/Content",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "Dictionary",
        "path":"/sitecore/content/sitecore-jss-app/sitecore-jss-app Dictionary",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "Layouts-Project",
        "path":"/sitecore/layout/Layouts/Project",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "Renderings-Feature",
        "path":"/sitecore/layout/Renderings/Feature",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants",
        "rules": [
          {
            "path": "/Experience Accelerator",
            "scope": "Ignored"
          },
          {
            "path": "/JSS Experience Accelerator",
            "scope": "Ignored"
          }]
      },
      {
        "name": "Renderings-Project",
        "path":"/sitecore/layout/Renderings/Project",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "PlaceholderSettings",
        "path":"/sitecore/layout/Placeholder Settings/Project",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "MediaLibrary",
        "path":"/sitecore/media library",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants",
        "rules": [
          {
            "path": "/Experience Explorer",
            "scope": "Ignored"
          },
          {
            "path": "/Foundation",
            "scope": "Ignored"
          },
          {
            "path": "/System",
            "scope": "Ignored"
          },
          {
            "path": "/Base Themes",
            "scope": "Ignored"
          },
          {
            "path": "/Themes",
            "scope": "Ignored"
          }
        ]
      },
      {
        "name": "Branches-Feature",
        "path":"/sitecore/templates/Branches/Feature/Our Features",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "Templates-SVCommerce",
        "path":"/sitecore/templates/SVCommerce",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "Templates-Feature",
        "path":"/sitecore/templates/Feature",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants",
        "rules": [
          {
            "path": "/Experience Accelerator",
            "scope": "Ignored"
          },
          {
            "path": "/JSS Experience Accelerator",
            "scope": "Ignored"
          }]
      },
      {
        "name": "Templates-Project",
        "path":"/sitecore/templates/Project",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
      },
      {
        "name": "Store",
        "path":"/sitecore/Stores",
        "allowedPushOperations": "createUpdateAndDelete",
        "scope": "ItemAndDescendants"
